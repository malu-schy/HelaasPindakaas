        "foregroundImage": "./assets/img/appIcon.png",
        "backgroundColor": "#CCC6C0"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2d3d8367-2f54-4a5e-83b0-f2439aed9887"
      }
    },
    "notification": {
      "sound": false
